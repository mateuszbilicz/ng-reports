{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "Node16",
    "target": "es2021",
    "rootDir": "./",
    "outDir": "../../build/client-host",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true
  },
  "exclude": []
}
